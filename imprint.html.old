    <div id="footer">
      <div id="player">
        <audio id="audio">
        </audio>
        <div id="control">
          <div class="icon"  id="backward"  onclick="backward()"></div>
          <div class="icon"  id="stop"      onclick="stop()"></div>
          <div id="playlistContainer">
            <div class="label" id="currentTitle"></div>
            <div id="playlistSlideIn">
              <ul id="playlist">
                <li>Dolchsto&szlig;</li>
                <li>Neue Wege</li>
                <li>Wenn die Flut kommt</li>
              </ul>
            </div>
          </div>
          <div class="icon"  id="play"      onclick="play()"></div>
          <div class="icon"  id="pause"     onclick="pause()"></div>
          <div class="icon"  id="forward"   onclick="forward()"></div>
          <div class="label" id="currentTime">00:00</div>
          <input type="range" id="seek" min="0" max="100" value="0"
                 onchange="seek()"/>
          <div class="label" id="totalTime">00:00</div>
          <div id="volume">
            <div class="icon"  id="mute"      onclick="unmute()"></div>
            <div class="icon"  id="quiet"     onclick="mute()"></div>
            <div class="icon"  id="moderate"  onclick="mute()"></div>
            <div class="icon"  id="loud"      onclick="mute()"></div>
            <div id="volumeSlideIn">
              <div class="icon"  id="plus" onclick="volumeUp()"></div>
              <br />
              <input type="range" id="volumeRange" min="0" max="100" value="100"
                     orient="vertical" onchange="changeVolume()"/>
              <br />
              <div class="icon"  id="minus"     onclick="volumeDown()"></div>
            </div>
          </div>
        </div>
      </div>
